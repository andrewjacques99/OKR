{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "_artifactsLocation": {
      "type": "string",
      "defaultValue": "T:\\WORK"
    }
  },
  "variables": {
    "netinfTemplateFolder": "nestedtemplates",
    "netinfTemplateFileName": "netinf.json",
    "netinfTemplateParametersFileName": "netinf.parameters.json"
  },
  "resources": [
    {
      "name": "netinf",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-05-01",
      "resourceGroup": "rg-netinf-prod-001",
      "dependsOn": [],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('_artifactsLocation'), '/', variables('netinfTemplateFolder'), '/', variables('netinfTemplateFileName'))]",
          "contentVersion": "1.0.0.0"
        },
        "parametersLink": {
          "uri": "[concat(parameters('_artifactsLocation'), '/', variables('netinfTemplateFolder'), '/', variables('netinfTemplateParametersFileName'))]",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ],
  "outputs": {
  }
}